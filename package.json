{
  "name": "@drivetech/dmm-parser",
  "version": "1.0.0",
  "description": "Parser for latitude and longitude in DMM format",
  "main": "src",
  "scripts": {
    "lint": "eslint . --fix",
    "format": "prettier-standard '{src,test}/**/*.js'",
    "commit": "commit",
    "test": "nyc mocha"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Drivetech/dmm-parser.git"
  },
  "keywords": [
    "nmea",
    "parser",
    "dmm",
    "coordinates"
  ],
  "author": "Leonardo Gatica <lgatica@protonmail.com> (https://about.me/lgatica)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Drivetech/dmm-parser/issues"
  },
  "homepage": "https://github.com/Drivetech/dmm-parser#readme",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@commitlint/prompt-cli": "7.2.1",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.7",
    "@semantic-release/github": "5.2.8",
    "@semantic-release/npm": "5.1.3",
    "chai": "4.2.0",
    "eslint": "5.11.1",
    "eslint-config-jsdoc-strict": "1.0.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "3.15.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-standard": "4.0.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.0",
    "mocha": "5.2.0",
    "npm-github-config": "2.0.0",
    "nyc": "13.1.0",
    "nyc-config-common": "1.0.1",
    "prettier-standard": "8.0.1",
    "semantic-release": "15.13.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "standard",
      "jsdoc-strict",
      "plugin:promise/recommended",
      "plugin:security/recommended"
    ],
    "plugins": [
      "promise",
      "security"
    ],
    "rules": {
      "no-console": [
        "error"
      ],
      "require-await": [
        "error"
      ]
    }
  },
  "eslintIgnore": [
    "coverage",
    "example.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "linters": {
      "{src,test}/**/*.js": [
        "eslint --fix",
        "prettier-standard",
        "git add"
      ]
    }
  },
  "tonicExampleFilename": "example.js",
  "release": {
    "extends": "npm-github-config"
  },
  "nyc": {
    "extends": "nyc-config-common"
  },
  "publishConfig": {
    "access": "public"
  },
  "renovate": {
    "automerge": "minor",
    "extends": [
      "config:js-lib"
    ]
  }
}
